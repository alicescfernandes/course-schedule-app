<?xml version='1.0' encoding='utf-8'?>
<widget id="com.af.horario" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hor&#xE1;rio CET24</name>
    <description>
Horario do CET24</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
     <preference name="StatusBarOverlaysWebView" value="false" />
     <preference name="StatusBarBackgroundColor" value="#4ff9ad" />
     <preference name="prerendered-icon" value="false" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
      <icon src="www/res/icon.png"/>
    <platform name="android">
        <icon src="www/res/icon.png" density="ldpi" />
        <icon src="www/res/icon.png" density="mdpi" />
        <icon src="www/res/icon.png" density="hdpi" />
        <icon src="www/res/icon.png" density="xhdpi" />
        <icon src="www/res/icon.png" density="xxhdpi" />
        <icon src="www/res/icon.png" density="xxxhdpi" />
        <allow-intent href="market:*" />
    </platform>
   
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
