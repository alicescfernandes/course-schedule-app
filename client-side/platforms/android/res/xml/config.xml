<?xml version='1.0' encoding='utf-8'?>
<widget id="com.af.horario" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Hor√°rio CET24</name>
    <description>
Horario do CET24</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/res/icon.png" />
    <icon density="ldpi" src="www/res/icon.png" />
    <icon density="mdpi" src="www/res/icon.png" />
    <icon density="hdpi" src="www/res/icon.png" />
    <icon density="xhdpi" src="www/res/icon.png" />
    <icon density="xxhdpi" src="www/res/icon.png" />
    <icon density="xxxhdpi" src="www/res/icon.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#4ff9ad" />
    <preference name="prerendered-icon" value="false" />
</widget>
